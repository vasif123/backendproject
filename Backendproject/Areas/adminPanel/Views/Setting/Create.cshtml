@model Setting
@{
    ViewData["Title"] = "Create";
}


<div class="main-panel">
    <h1>Create setting</h1>
    <form class="mt-3" method="post">
        <div>
            <div class="text-danger" asp-validation-summary="ModelOnly"></div>
            <label>Key:</label><br />
            <input asp-for="Key" class="form-control" /><br />
            <span class="text-danger" asp-validation-for="Key"></span>
        </div>
        <div>
            <label>Value:</label><br />
            <input asp-for="Value" class="form-control" /><br />
            <span class="text-danger" asp-validation-for="Value"></span>
        </div>
        <div class="mt-2">
            <button class="btn btn-success" type="submit">Create</button>
        </div>
    </form>
</div>